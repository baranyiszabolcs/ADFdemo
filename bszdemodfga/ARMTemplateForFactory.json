{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "bszdemodfga"
		},
		"AzureBlobStrOnpremStage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStrOnpremStage'"
		},
		"AzureDatabricksdemoLS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksdemoLS'"
		},
		"localFileLs_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'localFileLs'"
		},
		"AzureBlobTarget_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "stagefile.txt"
		},
		"AzureBlobTarget_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "onpremcopy"
		},
		"LocalDatafile_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@concat('QID341957_20170426_40216_',dataset().fileid,'.txt')"
		},
		"LocalDatafile_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "2015"
		},
		"localFileLs_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "szbarany"
		},
		"triggertv_properties_pipeline_parameters_outputfilename": {
			"type": "string",
			"defaultValue": "ofilex"
		},
		"bszssis_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "bszdfdemosql.database.windows.net"
		},
		"bszssis_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "szbarany"
		},
		"bszssis_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/copyOnpremData')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "CopyOnpremtoBlob",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "FileSystemSource",
								"recursive": true
							},
							"sink": {
								"type": "BlobSink"
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0
						},
						"inputs": [
							{
								"referenceName": "LocalDatafile",
								"type": "DatasetReference",
								"parameters": {
									"fileid": {
										"value": "@pipeline().parameters.fileId",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
								"referenceName": "AzureBlobTarget",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"parameters": {
					"fileId": {
						"type": "String",
						"defaultValue": "10"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/LocalDatafile')]",
				"[concat(variables('factoryId'), '/datasets/AzureBlobTarget')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dbrickPipe')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "dbrickcopyPYNB",
						"description": "python notebook",
						"type": "DatabricksNotebook",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": "/Users/szbarany@microsoft.com/sntdemo/dbrickcopy",
							"baseParameters": {
								"inputfile": {
									"value": "@pipeline().parameters.outputfilename",
									"type": "Expression"
								}
							}
						},
						"linkedServiceName": {
							"referenceName": "AzureDatabricksdemoLS",
							"type": "LinkedServiceReference"
						}
					}
				],
				"parameters": {
					"outputfilename": {
						"type": "String",
						"defaultValue": "ofile"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureDatabricksdemoLS')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobTarget')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "copy data rows to lob as temp",
				"linkedServiceName": {
					"referenceName": "AzureBlobStrOnpremStage",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureBlob",
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": true
					},
					"fileName": "[parameters('AzureBlobTarget_properties_typeProperties_fileName')]",
					"folderPath": "[parameters('AzureBlobTarget_properties_typeProperties_folderPath')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureBlobStrOnpremStage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/LocalDatafile')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "localFileLs",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"fileid": {
						"type": "String",
						"defaultValue": "10"
					}
				},
				"annotations": [],
				"type": "FileShare",
				"structure": [
					{
						"name": "Prop_0",
						"type": "String"
					},
					{
						"name": "Prop_1",
						"type": "String"
					},
					{
						"name": "Prop_2",
						"type": "String"
					},
					{
						"name": "Prop_3",
						"type": "String"
					},
					{
						"name": "Prop_4",
						"type": "String"
					},
					{
						"name": "Prop_5",
						"type": "String"
					},
					{
						"name": "Prop_6",
						"type": "String"
					},
					{
						"name": "Prop_7",
						"type": "String"
					},
					{
						"name": "Prop_8",
						"type": "String"
					},
					{
						"name": "Prop_9",
						"type": "String"
					},
					{
						"name": "Prop_10",
						"type": "String"
					},
					{
						"name": "Prop_11",
						"type": "String"
					},
					{
						"name": "Prop_12",
						"type": "String"
					},
					{
						"name": "Prop_13",
						"type": "String"
					},
					{
						"name": "Prop_14",
						"type": "String"
					},
					{
						"name": "Prop_15",
						"type": "String"
					},
					{
						"name": "Prop_16",
						"type": "String"
					},
					{
						"name": "Prop_17",
						"type": "String"
					},
					{
						"name": "Prop_18",
						"type": "String"
					}
				],
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": "|",
						"rowDelimiter": "\n",
						"nullValue": "\\N",
						"encodingName": null,
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"fileName": {
						"value": "[parameters('LocalDatafile_properties_typeProperties_fileName')]",
						"type": "Expression"
					},
					"folderPath": "[parameters('LocalDatafile_properties_typeProperties_folderPath')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/localFileLs')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStrOnpremStage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "stage for oprem data",
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStrOnpremStage_connectionString')]"
				},
				"connectVia": {
					"referenceName": "localselfhostedir",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/localselfhostedir')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDatabricksdemoLS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Databricks linked service",
				"annotations": [],
				"type": "AzureDatabricks",
				"typeProperties": {
					"domain": "https://northeurope.azuredatabricks.net",
					"accessToken": {
						"type": "SecureString",
						"value": "[parameters('AzureDatabricksdemoLS_accessToken')]"
					},
					"newClusterNodeType": "Standard_F4s",
					"newClusterNumOfWorker": "1",
					"newClusterVersion": "4.2.x-scala2.11"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/localFileLs')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "local file reader linked service",
				"annotations": [],
				"type": "FileServer",
				"typeProperties": {
					"host": "c:\\transfer\\",
					"userId": "[parameters('localFileLs_properties_typeProperties_userId')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('localFileLs_password')]"
					}
				},
				"connectVia": {
					"referenceName": "localselfhostedir",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/localselfhostedir')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/triggertv')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "tumbling vindow of databrcks",
				"annotations": [],
				"runtimeState": "Started",
				"pipeline": {
					"pipelineReference": {
						"referenceName": "dbrickPipe",
						"type": "PipelineReference"
					},
					"parameters": {
						"outputfilename": "[parameters('triggertv_properties_pipeline_parameters_outputfilename')]"
					}
				},
				"type": "TumblingWindowTrigger",
				"typeProperties": {
					"frequency": "Hour",
					"interval": 2,
					"startTime": "2018-09-05T20:45:00.000Z",
					"delay": "00:00:00",
					"maxConcurrency": 50,
					"retryPolicy": {
						"intervalInSeconds": 30
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/dbrickPipe')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/bszssis')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "West Europe",
						"nodeSize": "Standard_D1_v2",
						"numberOfNodes": 1,
						"maxParallelExecutionsPerNode": 1
					},
					"ssisProperties": {
						"catalogInfo": {
							"catalogServerEndpoint": "[parameters('bszssis_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]",
							"catalogAdminUserName": "[parameters('bszssis_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]",
							"catalogAdminPassword": {
								"type": "SecureString",
								"value": "[parameters('bszssis_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
							},
							"catalogPricingTier": "S0"
						},
						"edition": "Standard",
						"licenseType": "LicenseIncluded"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/localselfhostedir')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"description": "Self hosted IR download to local",
				"typeProperties": {}
			},
			"dependsOn": []
		}
	]
}