{
	"name": "dbrickPipe",
	"properties": {
		"activities": [
			{
				"name": "dbrickcopyPYNB",
				"description": "python notebook",
				"type": "DatabricksNotebook",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"notebookPath": "/sntdemo/dbrickcopy",
					"baseParameters": {
						"inputfile": {
							"value": "@pipeline().parameters.outputfilename",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricksdemoLS",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"outputfilename": {
				"type": "String",
				"defaultValue": "ofile"
			}
		}
	}
}